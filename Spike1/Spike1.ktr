    <connection>MySQLServer</connection>
    <sql>SELECT max.user_id zips_user_id, 
val.zip_id,
z.city,
z.statecode,
z.zipcode
FROM (SELECT user_id, MAX(location_id) location_id, MAX(primary_loc) primary_loc
	FROM users_locations 
	GROUP BY user_id) max, 
	users_locations val, zips z
WHERE max.user_id = val.user_id
	AND max.location_id = val.location_id
	AND z.zip_id = val.zip_id
	AND max.primary_loc = &quot;Y&quot;;</sql>
    <limit>100</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
